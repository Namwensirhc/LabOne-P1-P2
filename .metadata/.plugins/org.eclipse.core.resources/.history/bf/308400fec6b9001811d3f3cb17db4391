package Exercises;

public class aboutMe {

	public static void main(String[] args) {
		int birthYear = 1994;
		var job = "Works as an Artist";
		var birthMonth = "May";

		System.out.println("I was born in " + birthYear + ".");
		System.out.println("Chris " + job + ".");
		System.out.println("Chris was born in the Month of " + birthMonth + ".");

	}

}
